generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id          Int         @id@default(autoincrement())
  name        String      
  lastName    String  
  email       String      @unique
  password    String 
  cpf         String      @unique
  
  profile   Profile[]
}

model Profile{
  id        Int         @id@default(autoincrement())
  title     String      
  image     String      
  userId    Int
  user      User        @relation(fields:[userd],references:[id], onDelete:Cascade ,onUpdate: Cascade)
  
}
